    "expect": "^26.1.0",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "jest-junit": "^11.1.0",
    "lint-staged": "^10.2.13",
    "nodemon": "^2.0.4",
    "prettier": "2.1.1",
    "supertest": "^4.0.2",
    "xmlhttprequest": "^1.8.0"
  }
}
