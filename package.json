{
  "name": "Foxtail_API",
  "version": "0.0.1",
  "scripts": {
    "start": "NODE_ENV=staging nodemon -r esm src/start.js",
    "prostart": "NODE_ENV=production node -r esm src/start.js",
    "dev": "NODE_ENV=development nodemon -r esm src/start.js",
    "test": "NODE_ENV=staging jest --config ./jest.config.js --runInBand --forceExit",
    "watchtest": "nodemon --exec \"npm test\"",
    "build": "babel src -d dist --copy-files",
    "lint": "eslint ./",
    "prettier": "prettier --write ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && npm run lint",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "engines": {
    "node": ">=10.16.3"
  },
  "dependencies": {
    "@admin-bro/express": "^3.1.0",
    "@admin-bro/mongoose": "^1.1.0",
    "@sentry/node": "^5.20.1",
    "admin-bro": "^3.3.1",
    "async-lock": "^1.2.4",
    "authorizenet": "^1.0.7",
    "aws-sdk": "^2.721.0",
    "axios": "^0.19.2",
    "babel-preset-env": "^1.7.0",
    "bcrypt-nodejs": "^0.0.3",
    "compression": "^1.7.4",
    "connect-mongo": "^3.2.0",
    "connect-redis": "^5.1.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-brute": "^1.0.1",
    "express-brute-redis": "0.0.1",
    "express-graphql": "^0.11.0",
    "express-jwt": "^6.0.0",
    "express-mongo-sanitize": "^2.0.2",
    "express-rate-limit": "^5.1.3",
    "firebase-admin": "^9.0.0",
    "graphql": "^15.3.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-playground-middleware-express": "^1.7.18",
    "graphql-redis-subscriptions": "^2.2.1",
    "graphql-subscriptions": "^1.1.0",
    "helmet": "^3.23.3",
    "hpp": "^0.2.3",
    "imgix-core-js": "^2.3.1",
    "ioredis": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "mime": "^2.4.6",
    "moment": "^2.27.0",
    "mongoose": "^5.9.26",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "^1.10.0",
    "node-schedule": "^1.3.2",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "rate-limit-redis": "^2.0.0",
    "redis": "^3.0.2",
    "request-promise-native": "^1.0.9",
    "sanitize-filename": "^1.6.3",
    "shortid": "^2.2.15",
    "subscriptions-transport-ws": "^0.9.17",
    "uuid": "^8.3.2",
    "validator": "^13.1.1",
    "winston": "^3.3.3",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
